{
  "manifest_version": 2,
  "name": "Simple Url Switcher",
  "version": "2.001.1005",
  "description": "Switch url fluidly (for example youtube/soundcloud in keeping the progression) from your current device to another",
  "options_page": "settings.html",
  "permissions": [
    "activeTab","contextMenus","storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "URLSwitcher"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  }
}
